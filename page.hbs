{{!< default}}

{{#post}}
  <article class="post {{post_class}}">
    <div class="page__header" {{#if feature_image}} style="background-image:url('{{feature_image}}')"{{/if}}>
    <h1 class="page__title">{{title}}</h1>
    </div>
    <div class="page__content">
      {{content}}
    </div>
  </article>
{{/post}}